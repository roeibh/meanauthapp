{
    "extends": "tslint:recommended",
    "rules": {
        "no-console": [
            false
        ],
        "promise-function-async": true,
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ]
    }
}